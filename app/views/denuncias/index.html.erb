    <td><%= denuncia.municipio_hechos %></td>
    <td><%= denuncia.zona_hechos %></td>
    <td><%= denuncia.ambito %></td>
    <td><%= denuncia.fecha_agresion %></td>
    <td><%= denuncia.descripcion %></td>
