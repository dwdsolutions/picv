<h1>Listing sucursales</h1>

<table>
  <tr>
    <th>Direccion</th>
    <th>Municipio</th>
    <th>Institucion</th>
    <th>Telefono</th>
    <th>Email</th>
    <th>Longitud</th>
    <th>Latitud</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sucursales.each do |sucursal| %>
  <tr>
    <td><%= sucursal.direccion %></td>
    <td><%= sucursal.municipio %></td>
    <td><%= sucursal.institucion %></td>
    <td><%= sucursal.telefono %></td>
    <td><%= sucursal.email %></td>
    <td><%= sucursal.longitud %></td>
    <td><%= sucursal.latitud %></td>
    <td><%= link_to 'Show', sucursal %></td>
    <td><%= link_to 'Edit', edit_sucursal_path(sucursal) %></td>
    <td><%= link_to 'Destroy', sucursal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

