    <td><%= denuncia.condicion_agresor %></td>
    <td><%= denuncia.tipo_arma_agresor %></td>
    <td><%= denuncia.antecendente_criminal_agresor %></td>
    <td><%= link_to 'Show', denuncia %></td>
    <td><%= link_to 'Edit', edit_denuncia_path(denuncia) %></td>
    <td><%= link_to 'Destroy', denuncia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

